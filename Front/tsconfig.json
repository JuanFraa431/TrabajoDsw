{
  "compilerOptions": {
    "target": "ES6",
    "module": "es2022",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "mysql2/*": ["./node_modules/mysql2/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
